<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jeecms.jmhome.common.entity">
	<class name="Customer" table="customer">
		<id name="id" column="id" type="string">
			<generator class="uuid" />
		</id>
		<property name="phone" column="phone" type="string" />
		<property name="name" column="name" type="string" />
		<property name="password" column="password" type="string" />
		<property name="alipay" column="alipay" type="string" />
		<property name="oneJinType" column="one_jin_type" type="string" />
		<property name="twoJinType" column="two_jin_type" type="string" />
		<property name="gender" column="gender" type="java.lang.Boolean" />
	</class>
</hibernate-mapping>